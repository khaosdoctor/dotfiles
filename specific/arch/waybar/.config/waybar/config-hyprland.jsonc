[
  {
    "output": "DP-2",
    // "layer": "top", // Waybar at top layer
    // "position": "bottom", // Waybar position (top|bottom|left|right)
    "height": 42, // Waybar height (to be removed for auto height)
    "spacing": 0, // Gaps between modules (0px)
    // Choose the order of the modules
    "modules-left": [
      "hyprland/workspaces",
      "hyprland/submap",
      "wlr/taskbar",
    ],
    "modules-center": [
      "hyprland/window"
    ],
    "modules-right": [
      "clock",
      "custom/power"
    ],
    "custom/media": {
      "on-scroll-up": "playerctl -p spotify next",
      "on-scroll-down": "playerctl -p spotify previous",
      "on-click-middle": "playerctl -p spotify play-pause",
      "on-click": "hyprctl dispatch focuswindow class:spotify",
      "menu": "on-click-right",
      "smooth-scrolling-threshold": 5,
      "menu-file": "$HOME/.config/waybar/media_menu.xml",
      "exec-if": "pgrep spotify",
      "menu-actions": {
        "toggle": "playerctl -p spotify play-pause",
        "next": "playerctl -p spotify next",
        "previous": "playerctl -p spotify previous"
      },
      "return-type": "json",
      "max-length": 40,
      "format-icons": {
        "spotify": "ÔÜº ",
        "default": "üéú"
      },
      "escape": true,
      "exec": "$HOME/.config/waybar/mediaplayer.py --player spotify 2> /dev/null"
    },
    "wlr/taskbar": {
      "all-outputs": false,
      "format": "{icon}",
      "icon-theme": "Papirus-Dark",
      "tooltip": false,
      "on-click": "activate",
      "on-click-middle": "close"
    },
    "hyprland/submap": {
      "format": "<span style=\"italic\">{}</span>"
    },
    "hyprland/workspaces": {
      "all-outputs": false,
      "on-click": "activate",
      "on-scroll-up": "hyprctl dispatch workspace e+1",
      "on-scroll-down": "hyprctl dispatch workspace e-1"
    },
    "hyprland/window": {
      "separate-outputs": true
    },
    "idle_inhibitor": {
      "format": "{icon}",
      "format-icons": {
        "activated": "ÔÅÆ",
        "deactivated": "ÔÅ∞"
      }
    },
    "tray": {
      "show-passive-items": true,
      "spacing": 10
    },
    "clock": {
      "format": "{:%A %Y-%m-%d %H:%M}",
      "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
      "on-click": "/usr/bin/notion-calendar-electron"
    },
    "custom/power": {
      "format": "‚èª ",
      "tooltip": false,
      "menu": "on-click",
      "menu-file": "$HOME/.config/waybar/power_menu.xml", // Menu file in resources folder
      "menu-actions": {
        "shutdown": "systemctl poweroff",
        "reboot": "systemctl reboot",
        "suspend": "systemctl suspend",
        "logout": "hyprctl dispatch exit",
        "lock": "pidof hyprlock || hyprlock"
      }
    }
  },
